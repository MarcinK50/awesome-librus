{
  "name": "Awesome Librus",
  "version": "0.2",
  "author": "Kacper Åšlusarczyk",
  "manifest_version": 2,
  "description": "Modern Librus look",
  "homepage_url": "https://github.com/kcpru/awesome-librus",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_popup": "code/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://synergia.librus.pl/*"],
      "js": ["resources/jquery.min.js", "code/js.js"],
      "css": ["code/styles/css.css"]
    }
  ],
  "web_accessible_resources": ["resources/*"]
}
